  "keywords": [
    "chrome-extension",
    "network-monitoring",
    "network-analysis",
    "packet-monitoring",
    "network-stats",
    "network-speed-testing",
    "floating-widget",
    "real-time-monitoring",
    "chrome",
    "extension",
    "monitoring",
    "network",
    "security",
    "performance",
    "analytics"
  ],
  "author": "Network Monitor Pro Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Cameloo1/network-monitor-pro.git"
  },
  "bugs": {
    "url": "https://github.com/Cameloo1/network-monitor-pro/issues"
  },
  "homepage": "https://github.com/Cameloo1/network-monitor-pro#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {},
  "dependencies": {},
  "manifest": {
    "version": 3,
    "permissions": [
      "storage",
      "tabs",
      "webRequest",
      "webRequestAuthProvider",
      "background",
      "activeTab",
      "scripting"
    ]
  }
